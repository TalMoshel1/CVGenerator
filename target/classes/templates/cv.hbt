<!DOCTYPE html>
<html lang="en">
<head>
  </head>
<body>
  <div class="content">
    <h1>My CV</h1>

 <section class="section experience">
   <h2>PROFESSIONAL EXPERIENCE</h2>
   {{#each workSection}}
     <h3>{{title}}</h3>
     <p>{{company}} - ({{period}})</p>
     <ul>
       {{#each responsibilities}}
         <li>{{this}}</li>
       {{/each}}
     </ul>
   {{/each}}
 </section>

<section class="section educations">
    <h2>EDUCATION</h2>
    {{#each educationSection}}
        {{{this}}}  {{/each}}
</section>

    </div>
</body>
</html>
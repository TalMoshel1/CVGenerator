<!DOCTYPE html>
<html lang="en">
<head>
     <meta charset="UTF-8" />
     <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Your Document Title Here</title>
</head>
<body>
  <div class="content">
    <section class="section personal-details">
      {{#if personalDetails}}
        <h2>{{personalDetails.name}}</h2> <ul>
          {{#if personalDetails.phone}}
            <li>CELL: {{personalDetails.phone}}</li>
          {{/if}}
          {{#if personalDetails.email}}
            <li>EMAIL: {{personalDetails.email}}</li>
          {{/if}}
          {{#if personalDetails.linkedIn}}
            <li>LINKEDIN: <a href="{{personalDetails.linkedIn}}">{{personalDetails.linkedIn}}</a></li>
          {{/if}}
          {{#if personalDetails.gitHub}}
            <li>GITHUB: <a href="https://github.com/{{personalDetails.gitHub}}">{{personalDetails.gitHub}}</a></li>
          {{/if}}
          <li>SUMMARY: {{personalDetails.summary}}</li>
        </ul>
                {{#if personalDetails.summary}}
                  <p><strong>SUMMARY:</strong> {{personalDetails.summary}}</p>
                {{/if}}
      {{/if}}
    </section>

 <section class="section experience">
   <h2>PROFESSIONAL EXPERIENCE</h2>
   {{#each workSection}}
     <h3>{{title}}</h3>
     <p>{{company}} - ({{period}})</p>
     <ul>
       {{#each responsibilities}}
         <li>{{this}}</li>
       {{/each}}
     </ul>
   {{/each}}
 </section>

<section class="section educations">
    <h2>EDUCATION</h2>
    {{#each educationSection}}
        {{{this}}}  {{/each}}
</section>

    </div>
</body>
</html>